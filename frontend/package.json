    "@hookform/resolvers": "^2.9.11",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@tanstack/react-query": "^4.24.6",
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.0.1",
    "lucide-react": "^0.323.0",
    "mermaid": "^10.0.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
